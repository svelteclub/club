{
  "private": true,
  "type": "module",
  "name": "@svelteclub/club",
  "version": "0.0.1",
  "description": "An alternative Svelte REPL",
  "repository": "github:svelteclub/club",
  "bugs": "https://github.com/svelteclub/club/issues",
  "homepage": "https://svelte.club",
  "contributors": [
    "Braden Wiggins <braden@fractal-hq.com> (https://fractal-hq.com)",
    "Ross MacPhee (https://github.com/rossyman)"
  ],
  "scripts": {
    "start": "svelte-kit dev",
    "build:prod": "svelte-kit build",
    "build:preview": "npm run build && svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.3.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.17.0"
  }
}

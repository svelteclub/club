{
    "private": true,
    "type": "module",
    "name": "@svelteclub/club",
    "version": "0.0.1",
    "description": "An alternative Svelte REPL",
    "repository": "github:svelteclub/club",
    "bugs": "https://github.com/svelteclub/club/issues",
    "homepage": "https://svelte.club",
    "contributors": [
        "Braden Wiggins <braden@fractal-hq.com> (https://fractal-hq.com)",
        "Ross MacPhee (https://github.com/rossyman)",
        "GHOST <ghostdevbusiness@gmail.com> (https://github.com/ghostdevv)"
    ],
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "preview": "npm run build && svelte-kit preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "run-s lint:*",
        "lint:prettier": "prettier --ignore-path .lintignore  --check --plugin-search-dir=. .",
        "lint:eslint": "eslint --ignore-path .lintignore .",
        "lint:style": "stylelint --ignore-path .lintignore '**/*.{css,scss,html,svelte}'",
        "format": "run-s format:*",
        "format:prettier": "prettier --ignore-path .lintignore --write --plugin-search-dir=. .",
        "format:eslint": "eslint --fix --ignore-path .lintignore .",
        "format:style": "stylelint --fix --ignore-path .lintignore ."
    },
    "devDependencies": {
        "@fontsource/fira-code": "^4.5.1",
        "@fontsource/rubik": "^4.5.0",
        "@sveltejs/kit": "next",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "autoprefixer": "^10.3.2",
        "date-fns": "^2.23.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-svelte3": "^3.2.0",
        "include-media": "^1.4.10",
        "npm-run-all": "^4.1.5",
        "prettier": "~2.2.1",
        "prettier-plugin-svelte": "^2.2.0",
        "rfs": "^9.0.5",
        "sass": "^1.38.1",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-prettier": "^1.2.0",
        "svelte": "^3.34.0",
        "svelte-check": "^2.0.0",
        "svelte-hamburgers": "^3.0.1",
        "svelte-preprocess": "^4.0.0",
        "tslib": "^2.0.0",
        "typescript": "^4.0.0"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=14.17.0"
    }
}